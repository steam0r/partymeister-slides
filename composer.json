{
  "name": "partymeister/slides",
  "description": "Partymeister slides and beam system package",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "partymeister/core": "0.6.*",
    "pusher/pusher-php-server": "^4.1",
    "php-webdriver/webdriver": "^1.8",
    "beyondcode/laravel-websockets": "^1.4"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Partymeister\\Slides\\": "src",
      "Tests\\Partymeister\\Slides\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Partymeister\\Slides\\Providers\\PartymeisterServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
